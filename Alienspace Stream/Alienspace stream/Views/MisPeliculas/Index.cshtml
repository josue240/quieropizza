@model List<AlienspaceBL.Peliculas>

@{
    ViewBag.Title = "Index";
}

<h2>Mis Peliculas</h2>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                Codigo
            </th>
            <th>
                Pelicula
            </th>
        </tr>
    </thead>

 @foreach (var pelicula in Model)
    {
        <tr>
            <td>@pelicula.Id</td>
            <td>@pelicula.Pelicula</td>
        </tr>

    }
</table>